{
  "name": "melyia-app",
  "version": "20.1.0",
  "description": "Application SaaS Dentaire avec IA - Landing + Auth + Chatbot",
  "type": "module",
  "license": "MIT",
  "scripts": {
    "dev": "concurrently \"npm run dev:landing\" \"npm run dev:app\"",
    "dev:landing": "vite --config vite.config.ts --port 5173 --host 0.0.0.0",
    "dev:app": "vite --config vite.config.ts --port 5174 --host 0.0.0.0",
    "dev:debug": "DEBUG=* npm run dev",
    "dev:network": "npm run dev -- --host 0.0.0.0",
    "build": "npm run build:both",
    "build:both": "concurrently \"npm run build:landing\" \"npm run build:app\"",
    "build:landing": "vite build --config vite.config.ts --mode landing",
    "build:app": "vite build --config vite.config.ts --mode app",
    "build:clean": "rm -rf dist && npm run build:both",
    "build:check": "npm run build:both && echo 'Build size:' && du -sh dist/*",
    "deploy:full": "npm run deploy:local-optimized",
    "deploy:enhanced": "npm run build:both && node deploy-local-enhanced.mjs",
    "deploy:local-optimized": "npm run build:both && node deploy-local-optimized.mjs",
    "deploy:quick": "node deploy-local-enhanced.mjs",
    "deploy:landing:github": "node deploy-webhook-landing.mjs",
    "deploy:app:github": "node deploy-webhook-app.mjs",
    "deploy:status": "systemctl status nginx --no-pager && pm2 status && df -h /var/www",
    "deploy:logs": "sudo tail -f /var/log/nginx/access.log /var/log/nginx/error.log",
    "deploy:info": "echo 'üåê Sites d√©ploy√©s:' && echo '  üìç Landing: https://dev.melyia.com' && echo '  üìç App: https://app-dev.melyia.com' && echo '  üìç API: https://app-dev.melyia.com/api' && echo '' && echo 'üöÄ Commandes optimis√©es v36:' && echo '  npm run deploy:full     # D√©ploiement ultra-rapide (1.2s)' && echo '  npm run deploy:quick    # Build + deploy express' && echo '  npm run deploy:status   # √âtat serveur complet' && echo '  npm run deploy:logs     # Logs Nginx temps r√©el' && echo '  npm run test:deploy     # Tests HTTP + API'",
    "test": "echo 'üß™ Tests unitaires...' && npx vitest run || echo '‚ö†Ô∏è Tests pas configur√©s - TODO'",
    "test:deploy": "echo 'üß™ Tests d√©ploiement...' && curl -I https://dev.melyia.com && curl -I https://app-dev.melyia.com && curl -s https://app-dev.melyia.com/api/health | head -1",
    "test:full": "npm run test:deploy && npm run security:audit && npm run monitor:all",
    "test:performance": "node test-optimization-final.mjs",
    "lint": "echo 'üîç ESLint check...' && npx eslint . --ext .ts,.tsx,.js,.jsx --max-warnings 0 || echo '‚ö†Ô∏è ESLint pas configur√© - TODO'",
    "type-check": "tsc --noEmit",
    "monitor:nginx": "sudo systemctl status nginx && sudo nginx -t",
    "monitor:pm2": "pm2 status && pm2 logs melyia-auth-dev --lines 10",
    "monitor:db": "sudo systemctl status postgresql && sudo -u postgres psql -c '\\l'",
    "monitor:disk": "df -h && du -sh /var/www/melyia/* | sort -h",
    "monitor:all": "npm run monitor:nginx && echo '' && npm run monitor:pm2 && echo '' && npm run monitor:db && echo '' && npm run monitor:disk",
    "security:disable": "node manage-security.mjs disable",
    "security:enable": "node manage-security.mjs enable",
    "security:deploy": "node manage-security.mjs deploy",
    "security:status": "node manage-security.mjs status",
    "security:audit": "npm audit && npm run security:status",
    "maintenance:cleanup": "npm run build:clean && pm2 restart melyia-auth-dev && npm run monitor:all",
    "maintenance:update": "npm update && npm run build:check && npm run test:deploy",
    "server:restart": "sudo systemctl restart nginx && pm2 restart all",
    "server:logs": "sudo journalctl -u nginx -f",
    "server:backup": "sudo tar -czf /tmp/melyia-backup-$(date +%Y%m%d-%H%M%S).tar.gz /var/www/melyia/",
    "tools:cleanup": "node audit/versions/v36/scripts/cleanup-obsolete-scripts.mjs",
    "tools:optimize": "node optimize-package-commands.mjs",
    "preview": "vite preview"
  },
